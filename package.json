{
  "name": "redux-form-validate",
  "version": "1.0.1",
  "description": "Validate function for redux-form. Type checking based on React.PropTypes.",
  "main": "dist/index.js",
  "dependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "devDependencies": {},
  "scripts": {
    "lint": "eslint src",
    "prepublish": "npm run lint && npm test && node_modules/.bin/babel src --out-dir dist",
    "test": "node_modules/.bin/babel-node node_modules/mocha/bin/_mocha",
    "test:cover": "node_modules/.bin/babel-node node_modules/isparta/bin/isparta cover --report text --report html node_modules/mocha/bin/_mocha -- --reporter spec",
    "test:watch": "node_modules/.bin/babel-node node_modules/mocha/bin/_mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motoedie/redux-form-validate.git"
  },
  "keywords": [
    "react",
    "redux",
    "redux-form",
    "redux-form-validate",
    "validate",
    "validation"
  ],
  "author": "Ladislav Starek <ladislav.starek@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motoedie/redux-form-validate/issues"
  },
  "homepage": "https://github.com/motoedie/redux-form-validate#readme"
}
